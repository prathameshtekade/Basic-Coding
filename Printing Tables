package Stack;
import java.util.Scanner;

public class StackDemo {

	public static void main(String[] args) {
		int n,Total;
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter a Number");
		 n=sc.nextInt();
		
		 for(int i=1;i<=10;i++)
 {
	Total=n*i;
	 System.out.println(n+"*"+i+"="+Total);

 }

	} 
}
